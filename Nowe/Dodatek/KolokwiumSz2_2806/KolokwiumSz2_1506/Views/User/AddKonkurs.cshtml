
@model KonkursAddModel
@{
    ViewData["Title"] = "Konkurs";
}

<h2>Dodaj Konkurs</h2>
<form method="post">
    <div class="form-group">
        <label asp-for="Nazwa"> Nazwa</label>
        <input asp-for="Nazwa" type="text" class="form-control" />
        <span asp-validation-for="Nazwa" class="text-danger"></span>
    </div>

    <button type="submit" class="btn btn-outline-primary float-right">Zapisz</button>
</form>